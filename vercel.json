{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/index.py"
    },
    {
      "src": "/health",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "BOT_TOKEN": "@bot_token",
    "HF_SPACES_URL": "@hf_spaces_url",
    "MAIN_CHANNEL": "@main_channel",
    "LOG_CHANNEL": "@log_channel",
    "OWNER": "@owner"
  }
}
